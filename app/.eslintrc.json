{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "no-console": "off",
        "linebreak-style": "off",
        "strict": "off",
        "lines-around-directive": "off",
        "no-unused-vars": "off",
        "import/no-dynamic-require": "off",
        "global-require": "off",
        "import/no-extraneous-dependencies": "off"
    }
}
